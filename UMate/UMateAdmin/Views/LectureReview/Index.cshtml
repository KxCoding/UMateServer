@model IEnumerable<UMateModel.Entities.UMateBoard.LectureReview>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LectureInfoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assignment)
