@model IEnumerable<UMateModel.Entities.Place.Place>

@{ ViewData["Title"] = "Index"; }

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PlaceId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.University)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.District)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.PlaceId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Type)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.University.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.District)
    </td>
    <td>
        <a asp-action="Edit" asp-route-id="@item.PlaceId">Edit</a> |
        <a asp-action="Details" asp-route-id="@item.PlaceId">Details</a> |
        <a asp-action="Delete" asp-route-id="@item.PlaceId">Delete</a>
    </td>
</tr>
        }
    </tbody>
</table>
<div>
    @Html.PagedListPager((IPagedList)Model, page => Url.Action("Index", new { page = page }))
</div>
