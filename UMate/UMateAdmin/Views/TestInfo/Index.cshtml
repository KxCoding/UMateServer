@model IEnumerable<UMateModel.Entities.UMateBoard.TestInfo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TestInfoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LectureInfo.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Semester)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TestType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TestStrategy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QuestionTypes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedAt)
            </th>
        
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TestInfoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LectureInfo.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Semester)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestStrategy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuestionTypes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
          
            <td>
                <a asp-action="Edit" asp-route-id="@item.TestInfoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TestInfoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TestInfoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
